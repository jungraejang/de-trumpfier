{
  "name": "De-Trumpfier",
  "version": "0.1",
  "description": "Removes CheetoFace and improve browsing experience",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["assets/js/contentScript.js"]
    }
  ],
  "background": {
    "scripts": ["assets/js/background.js"],
    "persistent": false
  },
  "permissions": [
    "https://some-random-api.ml/*"
  ]
}
